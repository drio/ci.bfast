html
  head
    title bfast-ci
    link(rel='stylesheet', href='style.css')
  body
  table
    tr
      td date
      td bfast Darwin
      td bfast lucid32
      td bfast lucid64
      td bfast-bwa darwin
      td bfast-bwa lucid32
      td bfast-bwa lucid64
    - each entry, ts in entries
      tr 
        td= ts
        td= entry.Darwin.bfast
        td= entry.lucid32.bfast
        td= entry.lucid64.bfast
        td= entry.Darwin['bfast-bwa']
        td= entry.lucid32['bfast-bwa']
        td= entry.lucid64['bfast-bwa']
